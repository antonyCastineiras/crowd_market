# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/public/system/images/files/*

# Ignore Byebug command history file.
.byebug_history

/public/system/posts/images/
/public/system/users/images/
/public/images

# Don't check in credentials 
config/env.yml